#!/bin/bash

# The below incantation sets SRC_ROOT to be the canonicalized directory of this script
SRC_ROOT=$(
cd -P -- "$(dirname -- "$0")" &&
printf '%s\n' "$(pwd -P)/$(basename -- "$0")"
) || exit 1
SRC_ROOT=`dirname "$SRC_ROOT"`

# Move up into the actual source root directory
SRC_ROOT="$SRC_ROOT/../"

java -cp "$SRC_ROOT/lib/rvmonitor.jar":"$SRC_ROOT/lib/rt.jar":"$SRC_ROOT/lib/javamoptestsuite.jar:$SRC_ROOT/lib/javamop.jar:$SRC_ROOT/lib/logicrepository.jar:$SRC_ROOT/lib/*.jar" javamoptestsuite.Main -local "$SRC_ROOT/examples" $@

