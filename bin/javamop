#!/bin/bash

# The below incantation sets SRC_ROOT to be the canonicalized directory of this script
SRC_ROOT=$(
cd -P -- "$(dirname -- "$0")" &&
printf '%s\n' "$(pwd -P)/$(basename -- "$0")"
) || exit 1
SRC_ROOT=`dirname "$SRC_ROOT"`

# Move up into the actual source root directory
SRC_ROOT="$SRC_ROOT/../"

java -cp "$CLASSPATH:$SRC_ROOT/lib/rvmonitor.jar":"$SRC_ROOT/lib/rvmonitorrt.jar":"$SRC_ROOT/lib/javamop.jar":"$SRC_ROOT/lib/logicrepository.jar":"$SRC_ROOT/lib/plugins/ERE.jar":"$SRC_ROOT/lib/plugins/LTL.jar":"$SRC_ROOT/lib/plugins/PTLTL.jar":"$SRC_ROOT/lib/plugins/FSM.jar":"$SRC_ROOT/lib/plugins/CFG.jar":"$SRC_ROOT/lib/mysql-connector-java-3.0.9-stable-bin.jar" javamop.JavaMOPMain $@

